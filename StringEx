public class StringEx{
    public static void main(String[]args)
    {
        String name = "Yash Jain";
        System.out.println("Name is "+name );
        System.out.println("Length is "+name.length());
        System.out.println("Uppercase is "+name.toUpperCase());
        System.out.println("Lowecase is "+name.toLowerCase());
        System.out.println("Starts with Yash? " + name.startsWith("Yash"));
        System.out.println("Character at index 2: " + name.charAt(2));
        
    }
}
